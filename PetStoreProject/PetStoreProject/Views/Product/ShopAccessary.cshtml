                                    {
                                        <li class="page-item"><a class="page-link" onclick="NextPage(@(ViewBag.currentPage - 1),@ViewBag.pageSize)" href="javascript:void(0);">Previous</a></li>
                                    }
                                    @{
                                        var startPage = Math.Max(1, @ViewBag.currentPage - 2);
                                        var endPage = Math.Min(@ViewBag.totalPage, @ViewBag.currentPage + 2);
                                    }
                                    @if (@startPage > 1)
                                    {
                                        <li class="page-item"><a class="page-link" onclick="NextPage(1,@ViewBag.pageSize)" href="javascript:void(0);">1</a></li>
                                        if (@startPage > 2)
                                        {
                                            <li class="page-item"><a class="page-link" href="javascript:void(0);">...</a></li>
                                        }
                                    }
                                    @for (var i = startPage; i <= endPage; i++)
                                    {
                                        @if (@ViewBag.currentPage == i)
                                        {
                                            <li class="page-item active"><a class="page-link" href="javascript:void(0);">@i</a></li>
                                        }
                                        else
                                        {
                                            <li class="page-item"><a class="page-link" onclick="NextPage(@i,@ViewBag.pageSize)" href="javascript:void(0);">@i</a></li>
                                        }
                                    }
                                    @if (@ViewBag.page >= endPage + 1)
